{
  "name": "hoyo_gacha",
  "displayName": "HoYo.Gacha",
  "version": "1.1.11",
  "description": "An unofficial tool for managing and analyzing your miHoYo gacha records",
  "author": "lgou2w <lgou2w@hotmail.com>",
  "homepage": "https://github.com/lgou2w/HoYo.Gacha#readme",
  "repository": "https://github.com/lgou2w/HoYo.Gacha",
  "bugs": "https://github.com/lgou2w/HoYo.Gacha/issues",
  "license": "MIT OR Apache-2.0",
  "private": "true",
  "type": "module",
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a",
  "engines": {
    "node": "^20.19.0 || >= 22",
    "pnpm": "^10"
  },
  "scripts": {
    "prepare": "husky",
    "clean": "pnpm web:clean && pnpm backend:clean",
    "dev": "concurrently --raw -k \"pnpm:web:dev\" \"pnpm:backend:dev\"",
    "dev:tools": "cross-env REACT_DEVTOOLS=1 pnpm dev",
    "build": "pnpm web:build && pnpm backend:build",
    "build:offline": "pnpm web:build && pnpm backend:build:offline",
    "build:artifact": "node .scripts/build-artifact.cjs",
    "test": "pnpm web:test && pnpm backend:test",
    "lint": "pnpm web:lint && pnpm backend:lint",
    "web:clean": "rimraf dist",
    "web:dev": "cross-env NODE_ENV=development vite",
    "web:build": "cross-env NODE_ENV=production tsc && vite build",
    "web:preview": "cross-env NODE_END=production vite preview --port 1420",
    "web:test": "jest --verbose --passWithNoTests --config jest.config.cjs",
    "web:lint": "eslint --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0 --ignore-path .gitignore .",
    "web:lint:fix": "eslint --fix --ext .ts,.tsx --ignore-path .gitignore .",
    "backend:clean": "cargo clean",
    "backend:dev": "cross-env RUST_LOG=trace RUST_BACKTRACE=full tauri dev --features offline",
    "backend:build": "tauri build",
    "backend:build:offline": "tauri build --features offline",
    "backend:test": "cargo test",
    "backend:lint": "cargo check"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.72.7",
    "@fluentui/react-icons": "^2.0.315",
    "@fluentui/react-motion-components-preview": "^0.14.1",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-router": "^1.139.12",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-clipboard-manager": "^2.3.2",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "dayjs": "^1.11.19",
    "debounce": "^3.0.0",
    "i18next": "^25.7.1",
    "immer": "^11.0.1",
    "is-promise": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.67.0",
    "react-i18next": "^16.3.5",
    "react-virtuoso": "^4.16.1",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@griffel/tag-processor": "^1.0.14",
    "@griffel/vite-plugin": "^0.1.14",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tauri-apps/cli": "^2.9.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.25",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-testing-library": "^7.13.5",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
